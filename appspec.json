{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "AWS CloudFormation sample template that contains a single Lambda function behind an API Gateway based on Swagger",

  "Resources": {



    "LambdaTest": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Description": "A lambda one function B",
        "FunctionName": "LambdaTest",
        "Handler": "LambdaTest::LambdaTest.Function::FunctionHandler",
        "Role": { "Fn::GetAtt": [ "LambdaExecutionRole", "Arn" ] },
        "Runtime": "dotnetcore2.0"
      }
    },

    "LambdaExecutionRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": { "Service": [ "lambda.amazonaws.com" ] },
              "Action": [ "sts:AssumeRole" ]
            }
          ]
        },
        "ManagedPolicyArns": [ "arn:aws:iam::aws:policy/service-role/AWSLambdaBasicExecutionRole" ]
      }
    }
	}
